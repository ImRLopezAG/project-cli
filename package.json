{
  "name": "create-imr-app",
  "version": "0.0.12",
  "description": "Create web application with the imr stack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/t3-oss/create-imr-app.git",
    "directory": "cli"
  },
  "keywords": [
    "create-imr-app",
    "init.tips",
    "next.js",
    "t3-stack",
    "tailwind",
    "tRPC",
    "typescript"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "create-imr-app": "./dist/index.js"
  },
  "files": [
    "dist",
    "template",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    ".yarnrc.yml",
    "package.json"
  ],
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "typecheck": "tsc",
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist node_modules",
    "start": "node dist/index.js",
    "lint": "biome check --write --unsafe .",
    "release": "changeset version",
    "pub:beta": "pnpm build && npm publish --tag beta",
    "pub:next": "pnpm build && npm publish --tag next",
    "pub:release": "pnpm build && npm publish"
  },
  "dependencies": {
    "@clack/core": "^0.3.4",
    "@clack/prompts": "^0.6.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "chalk": "5.2.0",
    "commander": "^10.0.1",
    "execa": "^7.2.0",
    "fs-extra": "^11.2.0",
    "gradient-string": "^2.0.2",
    "jsonc-parser": "^3.3.1",
    "ora": "6.3.1",
    "sort-package-json": "^2.10.0"
  },
  "devDependencies": {
    "@libsql/client": "^0.15.14",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/react-query": "^5.87.1",
    "@tanstack/react-start": "^1.131.35",
    "@trpc/client": "11.5.0",
    "@trpc/next": "11.5.0",
    "@trpc/react-query": "11.5.0",
    "@trpc/server": "11.5.0",
    "@types/fs-extra": "^11.0.4",
    "@types/gradient-string": "^1.1.6",
    "@types/node": "^24.3.1",
    "@vitejs/plugin-react": "^5.0.2",
    "better-auth": "^1.3.8",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "next": "^15.5.2",
