    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "superjson": "^2.2.2",
    "tailwindcss": "^4.1.13",
    "tsup": "^8.5.0",
    "type-fest": "^4.41.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.4",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.1.5"
